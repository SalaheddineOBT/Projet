<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/back_account" />
    <item>
        <rotate android:fromDegrees="-90.0"
            android:pivotX="60.99999%"
            android:pivotY="70.0%" >
            <shape android:shape="rectangle">
                <corners android:radius="1dp" />
                <size android:width="20dp" android:height="50dp" />
                <solid android:color="@color/white" />
            </shape>
        </rotate>
    </item>
</layer-list>